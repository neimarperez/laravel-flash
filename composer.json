{
    "name": "neimarperez/laravel-flash",
    "type": "library",
    "description": "Flash messages for Laravel 5.",
    "keywords": [
        "neimarperez",
        "laravel-flash"
    ],
    "homepage": "https://github.com/neimarperez/laravel-flash",
    "license": "MIT",
    "authors": [{
        "name": "NeimarPerez",
        "email": "neimar.sistemas@gmail.com",
        "role": "Developer"
    }],
    "require": {
        "php": "^5.6 || ^7.0",
        "neimarperez/laravel-service-provider": "^0.1"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "NeimarPerez\\Flash\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "NeimarPerez\\Tests\\Flash\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "master"
        }
    }
}
